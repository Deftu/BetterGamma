{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${mod_version}",
    "name": "${mod_name}",
    "description": "",
    "authors": [
        "Deftu"
    ],
    "contact": {
        "issues": "https://github.com/Deftu/BetterGamma/issues",
        "sources": "https://github.com/Deftu/BetterGamma"
    },
